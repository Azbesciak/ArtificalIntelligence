(define (problem hanoi)
	(:domain hanoi-tower)
	(:objects x y z a b c d e)
	(:init
		(tower x)
		(tower y)
		(tower z)
		(disk a)
		(disk b)
		(disk c)
		(disk d)
		(disk e)
		(smaller a b)
		(smaller a c)
		(smaller a d)
		(smaller a e)
		(smaller b c)
		(smaller b d)
		(smaller b e)
		(smaller c d)
		(smaller c e)
		(smaller d e)
		(on a x)
		(on b x)
		(on c x)
		(on d x)
		(on e x)
	)
	(:goal
		(and
			(on a z)
			(on b z)
			(on c z)
			(on d z)
			(on e z)
		)
	)
)